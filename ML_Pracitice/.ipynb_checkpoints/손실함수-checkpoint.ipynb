    "#평균 제곱 오차\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "# y, t 는  numpy 배열\n",
    "def mean_squared_error(y, t):\n",
    "    return 0.5 * np.sum((y-t)**2)"
   ]
