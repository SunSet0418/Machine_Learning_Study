    "def softmax(a):\n",
    "    c = np.max(a)\n",
    "    exp_a = np.sum(a-c)\n",
    "    sum_exp_a = np.sum(exp_a)\n",
    "    y = exp_a / sum_exp_a\n",
    "\n",
    "    return y"
   ]
